规则("_Blood")
{
	事件
	{
		子程序;
		_Blood;
	}

	动作
	{
		移除玩家的生命池(事件玩家.id_player[0]);
		为玩家添加生命池(事件玩家, 生命值, 事件玩家.property_player[1], 真, 真);
		事件玩家.id_player[0] = 最后创建的生命池;
	}
}