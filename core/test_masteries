规则("_Initialize_Property")
{
	事件
	{
		子程序;
		_Initialize_Property;
	}

	动作
	{
		事件玩家.property_player[0] = 随机整数(1, 10);
		"最大生命值"
		事件玩家.property_player[1] = 100;
		设置最大生命值(事件玩家, 事件玩家.property_player[0]);
		"生命池"
		事件玩家.property_player[2] = 事件玩家.property_player[1] * 随机整数(10, 40);
		调用子程序(_Blood);
		"攻击"
		事件玩家.property_player[2] = 随机整数(80, 120);
		设置造成伤害(事件玩家, 事件玩家.property_player[3]);
		"速度"
		事件玩家.property_player[3] = 随机整数(95, 120);
		设置移动速度(事件玩家, 事件玩家.property_player[4]);
		"造成治疗"
		事件玩家.property_player[4] = 随机整数(80, 120);
		设置造成治疗(事件玩家, 事件玩家.property_player[6]);
		"受到治疗"
		事件玩家.property_player[5] = 随机整数(80, 120);
		设置受到治疗(事件玩家, 事件玩家.property_player[5]);
		"防御"
		事件玩家.property_player[6] = 100;
		设置受到伤害(事件玩家, 事件玩家.property_player[7]);
	}
}

